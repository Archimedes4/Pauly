{
  "navigationFallback": {
    "rewrite": "index.html",
    "exclude": ["/assets/*"]
  },
  "responseOverrides": {
    "400": {
      "rewrite": "/index.html"
    },
    "401": {
      "redirect": "/login",
      "statusCode": 302
    },
    "403": {
      "rewrite": "/custom-forbidden-page.html"
    },
    "404": {
      "rewrite": "/index.html"
    }
  }
}